<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android = "http://schemas.android.com/apk/res/android"
	xmlns:tools = "http://schemas.android.com/tools"
	package = "net.thearya.androidtestbed"
	>
	<uses-permission android:name = "android.permission.INTERNET"/>
	<application
		android:allowBackup = "false"
		android:icon = "@mipmap/icon_launcher"
		android:label = "@string/app_name"
		android:roundIcon = "@mipmap/icon_launcher_round"
		android:supportsRtl = "true"
		android:theme = "@style/AppTheme"
		tools:ignore = "GoogleAppIndexingWarning"
		>
		<activity android:name = ".ActivityMain">
			<intent-filter>
				<action android:name = "android.intent.action.MAIN"/>
				<category android:name = "android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name = ".ActivityAutoCompleteTextView"/>
		<activity android:name = ".ActivityGridView"/>
		<activity android:name = ".ActivityLinearLayout"/>
		<activity android:name = ".ActivityListView"/>
		<activity android:name = ".ActivityListViewCustomized"/>
		<activity android:name = ".ActivityListViewMultipleChoice"/>
		<activity android:name = ".ActivityRadioGroup"/>
		<activity android:name = ".ActivityRelativeLayout"/>
		<activity android:name = ".ActivityScrollView"/>
		<activity android:name = ".ActivitySpinner"/>
		<activity android:name = ".ActivityTableLayout"/>
		<activity android:name = ".ActivityWebView1"/>
		<activity android:name = ".ActivityWebView2"/>
		<activity android:name = ".ActivityWebView3"/>
	</application>
</manifest>
